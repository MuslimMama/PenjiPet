oled.blit(kitty, 0, 0)
    
    
    
while True:
    
    
    text2 = "counter: "
    text = text2 + str(counter)
    text_width = len(text) * 8  # 8 pixels per character
    x = 128 - text_width        # Right-aligned
    y = 64 - 8                  # Bottom row (each char is 8px tall)
        
    oled.text(text, x, y)
    oled.show()

    if button.value()== 0:
        counter += 1
    
    
        if counter%2 == 0:
            oled.fill(0)
            oled.blit(kittycough, 0, 0)
        else:
            oled.fill(0)
            oled.blit(kittycough2, 0, 0)
        time.sleep(1)
